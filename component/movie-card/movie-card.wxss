@import '/template/stars/stars.wxss';

.movie-card {
  width: inherit;
  min-width: 200rpx;
  overflow: hidden;
}

.movie-image {
  display: block;
  width: 100%;
  height: 280rpx;
  border-radius: 3px;
  overflow: hidden;
}

.movie-image image {
  display: block;
  width: 100%;
  height: 100%;
}

.movie-title {
  font-size: 14px;
}